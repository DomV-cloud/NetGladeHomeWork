{
  "Logging": {
    "LogLevel": { 
      "Default": "Error", 
      "Microsoft": "Warning" 
    },
    "Debug": { 
      "LogLevel": {
        "Default": "Information", 
        "Microsoft.Hosting": "Trace" 
      }
    },
    "EventSource": { 
      "LogLevel": {
        "Default": "Warning" 
      }
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "super-secret-key-super-secret-key", // minimum 256 bit
    "ExpiryMinutes": 60,
    "Issuer": "NetGlade",
    "Audience": "NetGlade"
  },
  "ConnectionStrings": {
    "sqlConnection": "Server=<NAME_OF_YOUR_DATABASE_SERVER>;Trusted_Connection=<TRUE_OR_FALSE>;TrustServerCertificate=<TRUE_OR_FALSE>;"
  }
}
